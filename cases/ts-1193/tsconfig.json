{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es6",
    "resolveJsonModule": true,
    "types": [],
    "incremental": true,
    "noErrorTruncation": true
  },
  "include": [
    "index.ts",
  ],
}
